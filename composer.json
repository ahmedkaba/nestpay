{
  "name": "payconn/nestpay",
  "description": "Payconn payment processing library for PHP",
  "keywords": [
    "nestpay",
    "est",
    "abank",
    "anadolu",
    "finans",
    "halk",
    "ing",
    "isbank",
    "seker",
    "turkekonomi",
    "teb",
    "ziraat"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Murat SAC",
      "email": "muratsac@mail.com"
    }
  ],
  "type": "library",
  "require": {
    "php": "^7.4.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "symfony/http-foundation": "^5.0",
    "payconn/common": "~1.4"
  },
  "autoload": {
    "psr-4": {
      "Payconn\\Nestpay\\": "src/Nestpay"
    },
    "classmap": [
      "src/Nestpay.php",
      "src/AkBank.php",
      "src/ABank.php",
      "src/AnadoluBank.php",
      "src/FinansBank.php",
      "src/HalkBank.php",
      "src/IngBank.php",
      "src/IsBank.php",
      "src/SekerBank.php",
      "src/TurkEkonomiBank.php",
      "src/ZiraatBank.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Payconn\\Nestpay\\Tests\\": "tests/Nestpay"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "phpstan/phpstan": "^0.12",
    "vimeo/psalm": "^3.9"
  }
}
